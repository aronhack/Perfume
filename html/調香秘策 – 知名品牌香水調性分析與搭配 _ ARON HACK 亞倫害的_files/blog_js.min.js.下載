var home_url=blog_data.home_url;jQuery(document).ready(function(a){var b=a('#post_img_zoom_container .modal-body img');a('.single article img').click(function(){if(!a(this).hasClass("zoom_disabled")){var c=a(this).attr('src');b.attr('src',c),b.attr('srcset',a(this).attr('srcset')),a('#post_img_zoom_btn').click()}}),a('#post_img_zoom_container .modal-body img').on('click',function(){a('#post_img_zoom_container .close').click()})})