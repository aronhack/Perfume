var ajax_url=ajax_object.ajax_url,load_more_switch=!0,scroll_head=!0,posts_per_page;jQuery(document).ready(function(a){posts_per_page=a('#load_more_posts').attr('posts_per_page'),a('#load_more_posts').on('click',function(a){a.preventDefault(),ajax_load_more_posts()})});function ajax_load_more_posts(){var a=$('#load_more_posts').attr('offset'),b=$('#content').attr('cur_lang'),c={action:'load_more_posts',offset:a,lang:b};$.get($('#load_more_posts').attr('ajax_url'),c,function(){$('#load_more_posts a').addClass('loading'),console.log(b)}).done(function(b){$('#post_archive').append(b),$('#load_more_posts').attr('offset',parseInt(a)+parseInt(posts_per_page)),$('#load_more_posts a').removeClass('loading'),load_more_switch=!0}).fail(function(){console.log("error")})}