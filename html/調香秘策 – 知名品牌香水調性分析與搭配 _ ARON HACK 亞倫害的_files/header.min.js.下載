var home_url=header_data.home_url,header=$('#master_header'),go_to_top=$('#go_to_top');jQuery(document).ready(function(a){var b=header.attr('disable_fixed');a(window).scroll(function(){var c=a(window).scrollTop();lastY=c,b==0&&lastY>0&&a(window).width()<=992?header.addClass('fixed'):header.removeClass('fixed'),c>200?(a('#go_to_top').removeClass('d-none'),a('#go_to_top').addClass('d-block')):(a('#go_to_top').removeClass('d-block'),a('#go_to_top').addClass('d-none'))}),a('#header_btn').click(function(){a(this).hasClass('collapsed')?a('#mobile_nav_bg').addClass('active'):a('#mobile_nav_bg').removeClass('active')}),a('#go_to_top a').on('click',function(b){return b.preventDefault(),a('html').animate({scrollTop:a('body').offset().top},'medium'),this})})